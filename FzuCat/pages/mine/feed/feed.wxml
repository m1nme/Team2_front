<!--pages/feed/feed.wxml-->
<view class="begin">
  <view>
    <input class="text01" auto-focus selection-start="2px" selection-end="4" placeholder="" />
  </view>
</view>
<block wx:for="{{cat_list}}" wx:key="id">
  <view class="line"></view>
  <view class='item'>
    <image class="image" src="{{item.catId}}"></image>
    <view class="title" >
      {{item.catId}}
      <text>\n</text>
      <text>{{item.time}} {{item.food}}</text>
    </view>
  </view>
</block>



